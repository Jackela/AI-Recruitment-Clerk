{
  "name": "shared-dtos",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared-dtos/src",
  "projectType": "library",
  "tags": [
    "scope:shared",
    "type:utils",
    "owner:platform-governance"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/shared-dtos",
        "tsConfig": "libs/shared-dtos/tsconfig.json",
        "packageJson": "libs/shared-dtos/package.json",
        "main": "libs/shared-dtos/src/index.ts"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/shared-dtos/jest.config.js",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/shared-dtos/**/*.ts"
        ]
      }
    }
  }
}
