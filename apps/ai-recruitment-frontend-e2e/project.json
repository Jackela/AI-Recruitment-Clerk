{
  "name": "ai-recruitment-frontend-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/ai-recruitment-frontend-e2e/src",
  "tags": ["type:e2e"],
  "implicitDependencies": [
    "ai-recruitment-frontend"
  ],
  "// targets": "to see all targets run: nx show project ai-recruitment-frontend-e2e --web",
  "targets": {
    "e2e": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/test-results",
        "{workspaceRoot}/playwright-report"
      ],
      "options": {
        "cwd": "apps/ai-recruitment-frontend-e2e",
        "commands": [
          "npx playwright test"
        ],
        "parallel": false,
        "env": {
          "E2E_SKIP_WEBSERVER": "false",
          "E2E_MANUAL_DEV_SERVER": "false",
          "E2E_USE_REAL_API": "false",
          "PLAYWRIGHT_BASE_URL": "http://localhost:4200",
          "DEV_SERVER_PORT": "4200"
        },
        "forwardAllArgs": true
      }
    }
  }
}
