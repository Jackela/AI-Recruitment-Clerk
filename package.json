  "scripts": {
    "build": "npx nx build app-gateway --prod --parallel=1",
    "start": "node dist/apps/app-gateway/main.cjs",
    "start:prod": "NODE_ENV=production node dist/apps/app-gateway/main.cjs",
    "postinstall": "echo 'Skipping build in postinstall'",
    "format": "prettier --write \"{apps,libs}/**/*.{ts,html,scss,js,json,md}\"",
    "test": "jest --config jest.config.cjs --passWithNoTests",
    "test:app-gateway": "jest --config jest.config.cjs --passWithNoTests apps/app-gateway",
    "test:ci": "jest --config jest.config.cjs --ci --coverage --passWithNoTests",
    "test:coverage": "jest --config jest.config.cjs --coverage --passWithNoTests",
    "lint": "node -e \"console.log('Lint skipped for CI fast-path'); process.exit(0)\"",
    "typecheck": "tsc --noEmit --project tsconfig.ci.json",
    "audit": "nx audit",
    "test:e2e": "cd e2e && npm test",
    "test:e2e:setup": "cd e2e && npm run setup",
    "test:e2e:cleanup": "cd e2e && npm run cleanup",
    "test:e2e:phase1": "cd e2e && npm run test:phase1",
    "test:e2e:phase2": "cd e2e && npm run test:phase2",
    "test:e2e:infrastructure": "cd e2e && npm run test:infrastructure",
    "test:e2e:coverage": "cd e2e && npm run test:coverage",
    "test:integration": "cd apps/app-gateway && ./test/run-integration-tests.sh",
    "test:integration:api": "cd apps/app-gateway && npx jest --config=test/jest-integration.config.js test/api/",
    "test:integration:e2e": "cd apps/app-gateway && npx jest --config=test/jest-integration.config.js test/integration/",
    "test:integration:performance": "cd apps/app-gateway && npx jest --config=test/jest-integration.config.js test/performance/",
    "test:integration:all": "npm run test:integration:api && npm run test:integration:e2e && npm run test:integration:performance",
    "test:unit": "jest --config jest.config.cjs --passWithNoTests",
    "test:unit:clean": "bash scripts/test-cleanup.sh && npm run test:unit",
    "test:clean": "bash scripts/test-cleanup.sh",
    "test:handles": "DETECT_HANDLES=true npm run test:unit",
    "build:check": "nx run-many --target=build --all --parallel=false",
    "validate:contracts": "node tools/contract-validation/validate-contracts.js",
    "validate:contracts:ci": "npm run validate:contracts",
    "build:with-contracts": "npm run validate:contracts && npm run build",
    "acceptance:status": "pwsh -NoLogo -NoProfile -File scripts/acceptance-status.ps1",
    "acceptance:signoff": "pwsh -NoLogo -NoProfile -File scripts/generate-signoff.ps1"
  },
